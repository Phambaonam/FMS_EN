var all = $('.cart-item')
var sum = 0
for (var i = 0; i < all.length; i++) {
    sum = parseInt(all[i].children['4'].innerText.replace(/[.]/g,''))
    sum += sum
}
console.log(sum)

var products = $('.price')
for (var i = 0; i< products.length; i++) {
    parseInt(products[i].children['0'].innerText).toLocaleString('vi')
    parseInt(products[i].children['1'].innerText).toLocaleString('vi')
}
